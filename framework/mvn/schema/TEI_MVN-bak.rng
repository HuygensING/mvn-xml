            <oneOrMore>
               <choice>
                  <ref name="mvnmodel.global"/>
                  <ref name="mvnmodel.graphicLike"/>
                  <ref name="mvnsurfaceGrp"/>
               </choice>
            </oneOrMore>
         </group>
         <ref name="mvnatt.global.attributes"/>
         <ref name="mvnatt.declaring.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="mvnatt.global.facs.attributes">
      <ref name="mvnatt.global.facs.attribute.facs"/>
   </define>
   <define name="mvnatt.global.facs.attribute.facs">
      <optional>
         <attribute name="facs">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(facsimile) points to all or part of an image which corresponds with the content of the element.</a:documentation>
            <list>
               <oneOrMore>
                  <ref name="mvndata.pointer"/>
               </oneOrMore>
            </list>
         </attribute>
      </optional>
   </define>
   <define name="mvnatt.global.change.attributes">
      <ref name="mvnatt.global.change.attribute.change"/>
   </define>
   <define name="mvnatt.global.change.attribute.change">
      <optional>
         <attribute name="change">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">points to one or more change elements documenting a state or revision campaign to which the element bearing this attribute and its children have been assigned by the encoder.</a:documentation>

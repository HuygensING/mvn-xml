      <element name="objectName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(name of an object) contains a proper noun or noun phrase used to refer to an object. [13.2.4. Object Names]</a:documentation>
         <ref name="mvnmacro.phraseSeq"/>
         <ref name="mvnatt.datable.attributes"/>
         <ref name="mvnatt.editLike.attributes"/>
         <ref name="mvnatt.global.attributes"/>
         <ref name="mvnatt.personal.attributes"/>
         <ref name="mvnatt.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="mvnobject">
      <element name="object">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains a description of a single identifiable physical object. [13.3.5. Objects]</a:documentation>
         <group>
            <oneOrMore>
               <ref name="mvnobjectIdentifier"/>
            </oneOrMore>
            <zeroOrMore>
               <ref name="mvnmodel.headLike"/>
            </zeroOrMore>
            <choice>
               <zeroOrMore>
                  <ref name="mvnmodel.pLike"/>
               </zeroOrMore>
               <empty/>
            </choice>
            <zeroOrMore>
               <choice>
                  <ref name="mvnmodel.noteLike"/>
                  <ref name="mvnmodel.biblLike"/>
        
        
               </choice>
            </zeroOrMore>
            <optional>
               <ref name="mvnobject"/>
            </optional>
         </group>
         <ref name="mvnatt.global.attributes"/>
         <ref name="mvnatt.sortable.attributes"/>
         <ref name="mvnatt.typed.attributes"/>
         <ref name="mvnatt.declaring.attributes"/>
         <ref name="mvnatt.docStatus.attributes"/>
         <ref name="mvnatt.canonical.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="mvnlistObject">
      <element name="listObject">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(list of objects) contains a list of descriptions, each of which provides information about an identifiable physical object. [13.3.5. Objects]</a:documentation>
         <group>
            <zeroOrMore>
               <ref name="mvnmodel.headLike"/>
            </zeroOrMore>
      
            <oneOrMore>
               <ref name="mvnmodel.objectLike"/>
            </oneOrMore>
            <zeroOrMore>
               <empty/>
            </zeroOrMore>
         </group>
         <ref name="mvnatt.global.attributes"/>
         <ref name="mvnatt.typed.attributes"/>
         <ref name="mvnatt.declarable.attributes"/>
         <ref name="mvnatt.sortable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="mvnobjectIdentifier">
      <element name="objectIdentifier">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(object identifier) groups one or more identifiers or pieces of locating information concerning a single object. [13.3.5. Objects]</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="mvnmodel.placeNamePart"/>
               <ref name="mvninstitution"/>
      
      
               <ref name="mvnidno"/>
               <ref name="mvnmsName"/>
               <ref name="mvnobjectName"/>
      
      
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TEI_MVN-objectIdentifier-objectIdentifier_minimal-constraint-report-13">
            <rule context="tei:objectIdentifier">
               <report test="not(count(*) gt 0)">An objectIdentifier must contain at minimum a single piece of locating or identifying information.</report>
            </rule>
         </pattern>
         <ref name="mvnatt.global.attributes"/>
         <empty/>
      </element>
   </define>
   <start>
      <choice>
         <ref name="mvnTEI"/>
         <ref name="mvncharDecl"/>
      </choice>
   </start>
</grammar>

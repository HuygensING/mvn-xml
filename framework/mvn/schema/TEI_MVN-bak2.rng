                  <ref name="mvntext"/>
                  <ref name="mvngroup"/>
               </choice>
               <zeroOrMore>
                  <choice>
                     <ref name="mvntext"/>
                     <ref name="mvngroup"/>
                     <ref name="mvnmodel.global"/>
                  </choice>
               </zeroOrMore>
            </group>
            <zeroOrMore>
               <ref name="mvnmodel.divBottom"/>
            </zeroOrMore>
         </group>

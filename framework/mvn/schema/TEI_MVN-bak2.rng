            </group>
            <group>
               <optional>
                  <group>
                     <ref name="mvnmodel.divBottomPart"/>
                  </group>
                  <zeroOrMore>
                     <choice>
                        <ref name="mvnmodel.divBottomPart"/>
                        <ref name="mvnmodel.global"/>
                     </choice>
                  </zeroOrMore>
               </optional>
            </group>
         </group>
         <ref name="mvnatt.global.attributes"/>
         <ref name="mvnatt.declaring.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="mvng">
      <element name="g">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(character or glyph) represents a glyph, or a non-standard character. [5. Characters, Glyphs, and Writing Modes]</a:documentation>
